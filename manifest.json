{
  "name": "Simple URL Redirector",
  "version": "0.2",
  "description": "Very simple URL redirector.",
  "background": { 
     "page":"bg.html"
     },
   "manifest_version": 2,
   "content_scripts": [
   {
     "matches": ["http://*/*", "https://*/*"],
     "js": ["content.js"]
   }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": ["tabs", "storage"]
}
